{
  "$schema": "https://turborepo.org/schema.json",

  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "src/**", "build/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "ladle": {
      "dependsOn": ["^ladle"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    }
  }
}
